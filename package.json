{
  "name": "test1",
  "version": "0.0.1",
  "description": "This project can be use as a starting point for developing your new Stellar Soroban SubQuery project",
  "main": "dist/index.js",
  "scripts": {
    "build": "subql codegen && subql build",
    "codegen": "subql codegen",
    "docker:build:node": "sudo docker build -f docker/subquery-node.Dockerfile -t subquery-node .",
    "docker:build:graphql": "sudo docker build -f docker/graphql-engine.Dockerfile -t graphql-engine .",
    "docker:build": "npm run docker:build:node && npm run docker:build:graphql",
    "start:local:subquery-node": "docker run --rm --name subquery-node -p 3000:3000 -v $(pwd):/app --env-file .env subquery-node"
  
  },
  "homepage": "https://github.com/subquery/stellar-subql-starter",
  "repository": "github:subquery/stellar-subql-starter",
  "files": [
    "dist",
    "schema.graphql",
    "project.yaml"
  ],
  "author": "SubQuery Team",
  "license": "MIT",
  "dependencies": {
    "@stellar/stellar-sdk": "latest",
    "@subql/common": "latest",
    "@subql/types-stellar": "latest",
    "@types/node": "^16.0.0",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@subql/cli": "latest",
    "@subql/testing": "latest",
    "@subql/types": "latest",
    "dotenv": "latest",
    "dotenv-cli": "^8.0.0",
    "typescript": "latest"
  }
}
